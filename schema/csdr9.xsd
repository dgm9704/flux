                    <xs:documentation source="Name" xml:lang="EN">BranchIdentification</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Identifies the code of the place of operation of the settlement internaliser (that is the branch), relating to the data that the report concerns.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SettlementInternaliserReportHeader1">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">SettlementInternaliserReportHeader1</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Specifies parameters of the report </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CreDtTm" type="ISODateTime">
