                    <xs:documentation source="Definition" xml:lang="EN">Repurchase transactions.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="OthrTxs" type="InternalisationData1">
                <xs:annotation>
                    <xs:documentation source="Name" xml:lang="EN">OtherTransactions</xs:documentation>
                    <xs:documentation source="Definition" xml:lang="EN">Covers any securities transactions type not covered as a dedicated type.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="TransactionOperationType4Code">
        <xs:annotation>
            <xs:documentation source="Name" xml:lang="EN">TransactionOperationType4Code</xs:documentation>
            <xs:documentation source="Definition" xml:lang="EN">Specifies the transaction operation action type.</xs:documentation>
